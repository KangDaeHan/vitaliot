<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <!-- Core Services -->

    <!-- vital-core-dms -->
    <entry key="vital-dms.mongo">mongodb://localhost:27017</entry>
    <entry key="vital-dms.security">https://localhost:8443/securitywrapper/rest</entry>
    <entry key="vital-dms.system.user">dmsuser</entry>
    <entry key="vital-dms.system.password">password</entry>
    <!-- /.vital-core-dms -->

    <!--vital-core-iot-data-adapter-->
    <!-- How to access the IoT data adapter. -->
    <entry key="vital-core-iot-data-adapter.base-url">http://localhost:8080/iot-data-adapter</entry>
    <!-- How to access the DMS. -->
    <!-- NOTE: This may later be replaced by some DMS-defined property. -->
    <entry key="vital-core-iot-data-adapter.vital-core-dms.base-url">http://localhost:8080/vital-core-dms</entry>
    <!-- How to connect to MongoDB. -->
    <entry key="vital-core-iot-data-adapter.mongo.uri">mongodb://localhost:27017</entry>
    <entry key="vital-core-iot-data-adapter.mongo.db">vital-iot-data-adapter</entry>
    <!-- How many workers under keeper. -->
    <entry key="vital-core-iot-data-adapter.keeper.workers">100</entry>
    <!--/.vital-core-iot-data-adapter-->

    <!--vital-core-iot-data-adapter-ppi-->
    <!-- How to access the IoT data adapter PPI. -->
    <entry key="vital-core-iot-data-adapter-ppi.base-url">http://localhost:8080/iot-data-adapter-ppi</entry>
    <!--/.vital-core-iot-data-adapter-ppi-->

    <!--vital-core-security-adapter-->
    <entry key="vital-core-security-adapter.idp-host">local.vital-iot-test.com</entry>
    <entry key="vital-core-security-adapter.idp-path">/vital-openam</entry>
    <entry key="vital-core-security-adapter.idp-port">8453</entry>
    <entry key="vital-core-security-adapter.proxy-host">local.vital-iot-test.com</entry>
    <entry key="vital-core-security-adapter.proxy-port">443</entry>
    <entry key="vital-core-security-adapter.sso-token">vitalAccessToken</entry>
    <entry key="vital-core-security-adapter.alt-token">vitalTestToken</entry>
    <!--/.vital-core-security-adapter-->

    <!--vital-core-security-snmp-exposer-->
    <entry key="vital-core-security-snmp-exposer.snmp-host">localhost</entry>
    <entry key="vital-core-security-snmp-exposer.snmp-port">8085</entry>
    <!--/.vital-core-security-snmp-exposer-->

    <!--vital-core-security-gateway-->
    <entry key="vital-core-security-gateway.proxy-host">local.vital-iot-test.com</entry>
    <entry key="vital-core-security-gateway.proxy-port">443</entry>
    <entry key="vital-core-security-gateway.proxy-ppi-path">/vital</entry>
    <entry key="vital-core-security-gateway.security-host">local.vital-iot-test.com</entry>
    <entry key="vital-core-security-gateway.security-port">8443</entry>
    <entry key="vital-core-security-gateway.username">ppigateway</entry>
    <entry key="vital-core-security-gateway.password">password</entry>
    <!--/.vital-core-security-gateway-->

    <!--vital-core-discovery-->
    <entry key="vital-core-discovery.dms">http://localhost:8080/vital-core-dms</entry>
    <entry key="vital-core-discovery.endpoint-address">http\://localhost:8080/vital-core-discovery</entry>
    <entry key="vital-core-discovery.endpoint-port">8080</entry>
    <!--/.vital-core-discovery-->

    <!--vital-core-filtering-->
    <!--/.vital-core-filtering-->

    <!--vital-core-cep-->
    <entry key="vital-core-cep.mongo.uri">mongodb://localhost:27017</entry>
    <entry key="vital-core-cep.mongo.db">vital-cep</entry>
    <entry key="vital-core-cep-ppi.base-url">http://localhost:8080/cep</entry>
    <entry key="vital-core-cep.mosquitto.uri">tcp://localhost:1883</entry>
    <entry key="vital-core-cep.ucep.path">/home/ucep</entry>
    <entry key="vital-core-cep.security">https://localhost:8443/securitywrapper/rest</entry>
    <entry key="vital-core-cep.dms">http://localhost:8080/vital-core-dms</entry>
    <!--/.vital-core-cep-->

    <!--vital-core-orchestrator-->
    <entry key="vital-core-orchestrator.mongo">mongodb://localhost:27017</entry>
    <entry key="vital-core-orchestrator.dms">http://localhost:8080/vital-core-dms</entry>
    <entry key="vital-core-orchestrator.security">http://localhost:8080/securitywrapper/rest</entry>
    <entry key="vital-core-orchestrator.sso-token">vitalAccessToken</entry>
    <entry key="vital-core-orchestrator.alt-token">vitalTestToken</entry>
    <entry key="vital-core-orchestrator.system.user">manplatform</entry>
    <entry key="vital-core-orchestrator.system.password">password</entry>
    <!--/.vital-core-orchestrator-->

    <!-- /. Core Services -->


    <!-- Governance and Management -->

    <!--vital-management-->
    <entry key="vital-management.mongo">mongodb://localhost:27017</entry>
    <entry key="vital-management.dms">http://localhost:8080/vital-core-dms</entry>
    <entry key="vital-management.iot-data-adapter">http://localhost:8080/vital-core-iot-data-adapter/rest</entry>
    <entry key="vital-management.security">http://localhost:8080/securitywrapper/rest</entry>
    <entry key="vital-management.security.sso-token">vitalAccessToken</entry>
    <entry key="vital-management.security.alt-token">vitalTestToken</entry>
    <entry key="vital-management.system.user">manplatform</entry>
    <entry key="vital-management.system.password">password</entry>
    <!--/.vital-management-->

    <!-- /.Governance and Management -->

</properties>
